{
	"name": "caml-select",
	"displayName": "Caml Select",
	"description": "Tool to select and run Caml functions",
	"publisher": "Cc618",
	"version": "0.0.4",
	"repository": {
		"type": "git",
		"url": "https://github.com/Cc618/Caml-Select"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.selectCamlFunction",
		"onCommand:extension.runCamlFunction"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.selectCamlFunction",
				"title": "Caml Select : Select Caml Function"
			},
			{
				"command": "extension.runCamlFunction",
				"title": "Caml Select : Run Caml Function"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
